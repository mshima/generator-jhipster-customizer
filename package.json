    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-5"
  ],
  "homepage": "https://github.com/mshima/generator-jhipster-customizer",
  "author": {
    "name": "Marcelo Shima",
    "email": "marceloshima@gmail.com",
    "url": "https://github.com/mshima"
  },
  "files": [
    "generators",
    "lib",
    "improver"
  ],
  "main": "lib/environment",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshima/generator-jhipster-customizer.git"
  },
  "dependencies": {
    "chalk": "3.0.0",
    "debug": "3.2.6",
    "dir-compare": "1.8.0",
    "ejs": "3.0.1",
    "fetch-repo-dir": "1.0.2",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "lodash": "4.17.15",
    "mem-fs-editor": "6.0.0",
    "require-dir-all": "0.4.15",
    "semver": "7.1.1",
    "yeoman-environment": "2.7.0",
    "yeoman-generator": "4.4.0"
  },
  "peerDependencies": {
    "generator-jhipster": "^6.3.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "ejs-lint": "0.3.0",
    "eslint": "5.14.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "jsdoc": "3.6.3",
    "mocha": "6.1.4",
    "prettier": "1.16.4",
    "shelljs": "0.8.3",
    "sinon": "8.0.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "npm run test:unit -- test/*.spec.js",
    "test:unit": "mocha --timeout 20000 --slow 0 --reporter spec",
    "jsdoc": "jsdoc --configure jsdoc-conf.json",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release"
  },
  "license": "Apache-2.0",
  "bugs": {
